    <questiontext format="html">
      <text><![CDATA[Která z následujících množin je zapsána jako <b>elementární obor integrace (EOI )vzhledem k ose x</b>?]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text></text>
    </generalfeedback>
    <defaultgrade>1.0000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <hidden>0</hidden>
    <idnumber></idnumber>
    <single>true</single>
    <shuffleanswers>true</shuffleanswers>
    <answernumbering>abc</answernumbering>
    <showstandardinstruction>1</showstandardinstruction>
    <correctfeedback format="html">
      <text>Vaše odpověď je správná.</text>
    </correctfeedback>
    <partiallycorrectfeedback format="html">
      <text>Vaše odpověď je částečně správná.</text>
    </partiallycorrectfeedback>
    <incorrectfeedback format="html">
      <text>Vaše odpověď je chybná.</text>
    </incorrectfeedback>
    <shownumcorrect/>
    <answer fraction="0" format="html">
      <text>\(\Omega:\displaystyle\begin{array}{l} 0\leq y\leq 3\\0 \leq x \leq 3 - y\end{array}\)</text>
      <feedback format="html">
        <text><![CDATA[<p>to není vzhledem k ose \(x\)<br></p>]]></text>
      </feedback>
    </answer>
    <answer fraction="100" format="html">
      <text>\(\Omega :\displaystyle\begin{array}{l}0\leq x\leq 3\\0 \leq y \leq 3 - x\end{array}\)</text>
      <feedback format="html">
        <text><![CDATA[<p>ANO!<br></p>]]></text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text>\(\Omega :\displaystyle\begin{array}{l}0\leq y\leq 3-x,\\ 0 \leq x \leq 3 - y\end{array}\)</text>
      <feedback format="html">
        <text><![CDATA[<p>přečtěte si skripta, str.56 (vydání 2016)<br></p>]]></text>
