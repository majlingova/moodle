gy = x0 * x0;

det = fx * gy - fy * gx;

dx = -(gy * fval - fy *gval) / det;
dy = -(-gx * fval + fx *gval) / det;
x1 = x0 + dx;
y1 = y0 + dy;

</text>
</varsglobal>
